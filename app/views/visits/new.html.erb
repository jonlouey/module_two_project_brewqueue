<h1> New Visit Form </h1>
<%= form_for(@visit) do |f|%>
<label>Event Name</label>
<%= f.text_field :event_name%><br>
<label>Date</label>
<%= f.date_field :visit_date%><br>
<label>Brew Queue ID</label>
<%= f.collection_select :user_brew_queue_id, @user_brew_queue, :id, :brewery_id%><br>

<%= f.submit%>
<%end%>